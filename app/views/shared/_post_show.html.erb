<h3><%= post.title %></h3>

<h4>Accsess Link</h4>
<div>
  <p><%= link_to post.url, post.url %></p>
</div>

<h4>Content</h4>
<div>
  <p><%= post.content %></p>
  <div>
    <% post.post_images.each do |img| %>
      <%= attachment_image_tag img, :image, :fill, 100, 100, format: 'jpeg', fallback: "no_image.jpg" %>
    <% end %>
  </div>
</div>



